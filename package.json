{
  "name": "blyzer",
  "version": "1.0.0",
  "description": "My well-made Nuxt.js project",
  "author": "NZXTUA",
  "private": true,
  "keywords": [
    "nuxtjs",
    "typescript"
  ],
  "scripts": {
    "dev": "nuxt-ts",
    "deva": "npm run dev --analyze",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "swag": "npm run generator-copy && npm run api-generate",
    "api-generate": "node ./node_modules/swagger-vue-generator/bin/generate-api.js -s ./swagger/swagger.json -d ./services/api.js -m api",
    "generator-copy": "cpx ./swagger/js-template/generator.js ./node_modules/swagger-vue-generator/src/2.0/generator-api/ -u",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@mdi/font": "^3.5.95",
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@saeris/vue-spinners": "^1.0.8",
    "cross-env": "^5.2.0",
    "vue-async-computed": "^3.6.1",
    "gsap": "^2.1.2",
    "moment": "^2.24.0",
    "nuxt-i18n": "^5.8.5",
    "nuxt-ts": "latest",
    "v-hotkey": "^0.3.0",
    "vue-flag-icon": "^1.0.6",
    "vue-property-decorator": "^7.3.0",
    "vue-tasty-burgers": "^1.1.4",
    "vuedraggable": "^2.19.2",
    "vuejs-noty": "^0.1.3",
    "vuetify": "^1.5.6",
    "vuetify-loader": "^1.2.0",
    "vuex-class": "^0.3.2",
    "vuex-persist": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^8.2.1",
    "cpx": "^1.5.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^1.18.9",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "swagger-vue-generator": "^1.0.6",
    "vue-eslint-parser": "^6.0.3"
  }
}
