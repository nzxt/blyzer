{
  "name": "blyzer",
  "version": "1.0.0",
  "description": "My well-made Nuxt.js project",
  "author": "NZXTUA",
  "private": true,
  "keywords": [
    "nuxtjs",
    "vuetifyjs",
    "typescript",
    "pug"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "swag": "yarn generator-copy && yarn api-generate",
    "api-generate": "node ./node_modules/swagger-vue-generator/bin/generate-api.js -s ./swagger/swagger.json -d ./services/api.js -m api",
    "generator-copy": "cpx ./swagger/js-template/generator.js ./node_modules/swagger-vue-generator/src/2.0/generator-api/ -u",
    "precommit": "yarn lint"
  },
