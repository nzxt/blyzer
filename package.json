    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "apexcharts": "^3.8.1",
    "core-js": "^2.6.9",
    "cross-env": "^5.2.0",
    "gsap": "^2.1.3",
    "konva": "^3.3.3",
    "moment": "^2.24.0",
    "nuxt": "^2.8.1",
    "nuxt-i18n": "^5.12.7",
